parse();

document.getElementById('send_email').addEventListener('click', email_send_event);

function email_send_event(){
    document.getElementById("send_email").removeEventListener("click", email_send_event);
    document.getElementById('send_email').style.display = 'none';
    
    email_send();
}

function email_send(){
    var email_to_be_sent = document.getElementById('formatted_target').value;
    var email_subject = 'Re: ' + document.getElementById('bod_subject').innerHTML;
    email_to_be_sent = email_to_be_sent + "\n---\nCary Network Management Center - Power/Transport\nO 800-251-6517\n7000 Weston Pkwy\nCary, NC 27513";
    google.script.run.send_email(email_subject, email_to_be_sent);
    window.alert("BOD-Report sent");
}
function parse(){
    var rawReport;
    var outputString = "";
    //var splits = document.getElementById('split_value').value;
    var splits = 1;
    var current_split = 1;
    var extra_entries_per_split;

    rawReport = document.getElementById('bod_raw');
    modReport = rawReport.cloneNode(true).getElementsByTagName('pre');
    
    
    if(modReport.length == 0) return;

    var site_list = findSites(modReport);

    var n_entries_per_split = Math.floor((modReport.length) / splits);
    extra_entries_per_split = (modReport.length-1) % splits ;
    console.log("Remainder: " + extra_entries_per_split);
    console.log("Entries per split: " + n_entries_per_split);

    for(i = 0; i < modReport.length; i++){
        modReport[i].innerHTML = modReport[i].innerHTML.replace(/\r?\n|\r/g, '')
        if(/<td>NOC<\/td><td>(\w+)<\/td>/.exec(modReport[i].innerHTML)[1].localeCompare("GPNOC") == 0){
            
            outputString += /<td>Location\(Site\)<\/td><td>(\w+) /.exec(modReport[i].innerHTML)[1];
            outputString += findLoc(modReport[i].innerHTML);
            outputString += findType(modReport[i].innerHTML);
            outputString += findPB(modReport[i].innerHTML) + "\n";
            console.log('\n\n\n'+(modReport[i].innerHTML));
            if(/<td>ETMS Ticket#<\/td><td>(.*?)<\/td>/.exec(modReport[i].innerHTML)[1].length == 0){
                if(/<td>VRepair Ticket#<\/td><td>(.*?)<\/td>/.exec(modReport[i].innerHTML)[1].length == 0){
                    console.log("No vR or Tkt\n" + modReport[i]);
                    outputString += findFlash(modReport[i].innerHTML, 0);
                }
                else{
                    console.log("vR Tkt\n" + modReport[i]);
                    outputString += "vRepair: " + /<td>VRepair Ticket#<\/td><td>(.*?)<\/td>/.exec(modReport[i].innerHTML)[1];
                    outputString += findFlash(modReport[i].innerHTML, 1);
                }
            }
            else{
                console.log("ETMS Tkt\n" + modReport[i]);
                outputString += " - ETMS: " + /<td>ETMS Ticket#<\/td><td>(.*?)<\/td>/.exec(modReport[i].innerHTML)[1];
                outputString += findFlash(modReport[i].innerHTML, 1);
            }

            outputString += "\n\n";
            console.log(outputString);
            document.getElementById('formatted_target').value = outputString;
        }
        else console.log("Non-GPNOC entry. Skipping");

        if(extra_entries_per_split){
            if(current_split % (n_entries_per_split+1));
            else{
                outputString += "----------------------------------------";
                console.log("-----");
                extra_entries_per_split -= 1;
            }
        }
        else{
            if(current_split % n_entries_per_split);
            else{
                outputString += "----------------------------------------";
                console.log("-----");
            }
        }
        current_split++;
        
    }
}

function findFlash(snip, dashOn){
    var outputString = "";

    if(dashOn == 1) outputString += "; ";

    if(/<b>OASIS Flash<\/b><\/td><\/tr><tr><td>ID<\/td><td>(.*?)<\/td>/.exec(snip)){
        outputString += "Flash: " + /<b>OASIS Flash<\/b><\/td><\/tr><tr><td>ID<\/td><td>(.*?)<\/td>/.exec(snip)[1];
        if(/<th>timeStamp \(GMT\)<\/th><th>comment<\/th><\/tr><tr><td>(?:.*?)<\/td><td>(.*?)<\/td>/m.exec(snip)){
            outputString += "\n" + /<th>timeStamp \(GMT\)<\/th><th>comment<\/th><\/tr><tr><td>(?:.*?)<\/td><td>(.*?)<\/td>/m.exec(snip)[1];
        }
    }

    outputString += "\n" + findBat(snip);

    return outputString;
}

function findBat(snip){
    var outputString = "";

    if(/<td>Est Batt Status<\/td><td>R=(.*?)\//.exec(snip)) 
        outputString += "Battery reserve remaining: " + /<td>Est Batt Status<\/td><td>R=(.*?)\//.exec(snip)[1] + " hours";
    console.log(outputString);
    return outputString;
}

function findType(snip){
    var outputString = "";

    if(/<td>Technology<\/td><td>(.+?)<\/td>/.exec(snip)) 
        outputString +=  " - TYPE: " + /<td>Technology<\/td><td>(.+?)<\/td>/.exec(snip)[1]; 
    console.log(outputString);
    return outputString;
}

function findLoc(snip){
    var outputString = "";

    if(/<td>State\/City<\/td><td>(.+?)\/(.*?)<\/td>/.exec(snip)[2]) 
        outputString +=  " - " + /<td>State\/City<\/td><td>(.+?)\/(.*?)<\/td>/.exec(snip)[2] + 
        ", " + /<td>State\/City<\/td><td>(.+?)\/(.*?)<\/td>/.exec(snip)[1];
    else if(/<td>State\/City<\/td><td>(.+?)\/(.*?)<\/td>/.exec(snip)[1]) 
        outputString +=  " - " + /<td>State\/City<\/td><td>(.+?)\/(.*?)<\/td>/.exec(snip)[1];
    console.log(outputString);
    return outputString;
}

function findPB(snip){
    var outputString = "";

    if(/<td>PB Level<\/td><td>(\d*?)<\/td>/.exec(snip)){ 
        if(/<td>PB Level<\/td><td>(\d*?)<\/td>/.exec(snip)[1] === "0"){
            outputString = " - PB?";
        }
        else{
            outputString += " - " + "PB" + /<td>PB Level<\/td><td>(\d*?)<\/td>/.exec(snip)[1];
        }
    }
    console.log(outputString);
    return outputString;
}
function findSites(modReport){
    console.log(modReport);
    var ret = new Array();
    for(i = 0; i < modReport.length; i++){
        var gpnoc_site_bool = /<td>NOC<\/td><td>(\w+)<\/td>/.exec(modReport[i].innerHTML)[1].localeCompare("GPNOC");
        if(gpnoc_site_bool == 0){
            var site = /<td>Location\(Site\)<\/td><td>(\w+) /.exec(modReport[i].innerHTML)[1];
            ret.push(site);
            console.log(site);
        }
    }
    return ret;
}

